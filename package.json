	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.9.0",
		"cubing": "^0.35.6",
		"gan-web-bluetooth": "^1.0.3",
		"hammerjs": "^2.0.8",
		"scramble-display": "^0.5.4",
		"scrambow": "^1.7.0",
